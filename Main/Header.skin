            RenderWithNoTag="true">

<Content>

<ecom:NoCookiePanel runat="server" id="pnlCookies" class="cookies navbar-fixed-bottom">
		<div class="cookies-content container">
            <div class="row">

                <div class="col-md-10 col-xs-12">
                <p class="titre">Utilisation des cookies</p>
		                <p style="color:#ffffff; margin: 0px 0px 7px; width: 96%;line-height:1em">%{res:MsgPopinCookies}.  <a href="/vie-privee.htm" class="btn-link"> En savoir plus </a>
                        </p>
                </div>
            
             

                <div class="col-xs-12 col-md-2 cookies-ok-button" style="padding-top:3rem">
		                <ecom:SetCookieLink runat="server" id="lnkCookies" class="btn btn-primary">   
                            Accepter
		                </ecom:SetCookieLink>
		                </div>
            </div>
        </div>



        <!-- evol/2 -->

</ecom:NoCookiePanel>


        <ecom:PanierJavascriptData runat="server" ID="hdnPanierResume" />

<header class="top1">
			<div class="barre-noire">
