					<div class="d-flex justify-content-between row">
							<div class="col-md-4">
<ecom:PanelPageSpecifique runat="server"  VisibiliteParDefaut="true" SaufPour="HomePage">
<strong><ecom:SiteNom runat="server" /></strong> : <ecom:SiteSlogan runat="server" />
</ecom:PanelPageSpecifique>
							</div>
              <div class="d-flex align-items-center  barre-noire-item col-md-8 justify-content-end">
									
							</div>
					  </div>
				</div>
      </div>
      <div class="nav-main">
			  <div class="container">
          <div class="row">
            <div class="col-7 px-0 d-flex">
              <div class="brand nav-main-item align-items-center d-flex ">
                <a href="/">
                  
                </a>
              </div>
              <ecom:Menu Cssclass="menu nav-main-item  align-items-center d-flex" Code="TOPMENU">
                <ItemTemplate>
                  <ecom:MenuLink CssClass="menu-item">
                      <span class="menu-img">
                          <ecom:MenuImage />
                      </span>
                      <ecom:MenuText CssClass="menu-txt">
                  </ecom:MenuLink>
                </ItemTemplate>
              </ecom:Menu>
            </div>
          </div>
        </div>
